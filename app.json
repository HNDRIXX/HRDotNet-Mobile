{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "name": "HRDotNet-Mobile",
    "slug": "HRDotNet-Mobile",
    "android": {
      "permissions": ["READ_EXTERNAL_STORAGE"]
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIFileSharingEnabled": true
      }
    }
  }
}

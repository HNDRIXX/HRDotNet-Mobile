{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "orientation": "portrait",
    "name": "HRDotNet-Mobile",
    "slug": "HRDotNet-Mobile",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hndrx022.HRDotNetMobile"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIFileSharingEnabled": true
      },
      "bundleIdentifier": "com.hndrx022.HRDotNetMobile"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "00a79da3-f5ea-460d-9c97-12a30d7a5bf5"
      }
    }
  }
}
